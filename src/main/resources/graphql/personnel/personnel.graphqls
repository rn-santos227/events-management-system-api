extend type Query {
  personnel(limit: Int, page: Int): [Personnel!]!
}

extend type Mutation {
  updatePersonnel(id: ID!, input: PersonnelUpdateInput!): Personnel!
}

type Personnel {
  id: ID!
  name: String
  contactNumber: String
  email: String
  role: String
  createdAt: String
  updatedAt: String
}

input PersonnelUpdateInput {
  fullName: String
  contactNumber: String
  email: String
  role: String
}
